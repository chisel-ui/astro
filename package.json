{
  "name": "@chisel-ui/astro",
  "version": "1.0.0",
  "description": "A collection of layout primitives for Astro projects.",
  "type": "module",
  "main": "./index.ts",
  "exports": {
    ".": "./index.ts",
    "./*": "./src/*"
  },
  "files": [
    "src",
    "index.ts"
  ],
  "keywords": [
    "astro-component",
    "withastro",
    "css+ui",
    "layout",
    "primitives"
  ],
  "scripts": {
    "dev": "astro --root e2e-fixture dev",
    "build": "astro --root e2e-fixture build",
    "preview": "astro --root e2e-fixture preview",
    "format": "prettier --ignore-path .gitignore .",
    "format:fix": "prettier --ignore-path .gitignore -w .",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "lint:fix": "eslint --fix --ignore-path .eslintignore --ext .js,.ts,.astro --fix .",
    "test:e2e": "playwright test e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chisel-ui/astro.git"
  },
  "author": {
    "name": "Tony Sullivan",
    "email": "tony.f.sullivan@outlook.com",
    "url": "https://tonysull.co"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chisel-ui/astro/issues"
  },
  "homepage": "https://chisel-ui.dev",
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@playwright/test": "^1.27.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "astro": "^1.5.2",
    "astro-component-tester": "^0.6.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-astro": "^0.20.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.7.0",
    "semantic-release": "^19.0.5"
  },
  "peerDependencies": {
    "astro": "^1.6.1"
  },
  "dependencies": {
    "@chisel-ui/core": "^1.1.5"
  }
}
